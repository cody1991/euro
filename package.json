{
  "name": "europe-travel-planner",
  "version": "1.0.0",
  "description": "欧洲旅游规划网站 - 武汉到欧洲20天行程规划",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "travel",
    "europe",
    "planner",
    "react",
    "node"
  ],
  "author": "Cody Tang",
  "license": "MIT"
}
