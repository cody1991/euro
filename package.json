{
  "name": "europe-travel-map",
  "version": "1.0.0",
  "description": "欧洲旅游地图 - 武汉到欧洲20天行程地图展示",
  "main": "server/index-sqlite.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index-sqlite.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index-sqlite.js"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^3.0.2",
    "sqlite3": "^5.1.6"
  },
  "keywords": [
    "travel",
    "europe",
    "map",
    "react",
    "node",
    "sqlite",
    "leaflet"
  ],
  "author": "Cody Tang",
  "license": "MIT"
}
