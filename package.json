{
  "name": "europe-travel-planner",
  "version": "1.0.0",
  "description": "欧洲旅游规划网站 - 武汉到欧洲20天行程规划",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "server:postgres": "nodemon server/index-postgres.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "start:postgres": "node server/index-postgres.js",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "mysql2": "^3.6.0",
    "nodemon": "^3.0.2",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6"
  },
  "keywords": [
    "travel",
    "europe",
    "planner",
    "react",
    "node"
  ],
  "author": "Cody Tang",
  "license": "MIT"
}
